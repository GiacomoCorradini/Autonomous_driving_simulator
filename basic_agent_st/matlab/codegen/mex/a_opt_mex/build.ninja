# CompilerName=mingw64
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/win64
matlabdir = C:/Program Files/MATLAB/R2022a
startdir = C:/Users/corra/Desktop/Università Trento/Master degree/2nd year/Intelligent vehicles and autonomous driving/Autonomous_driving_simulator/basic_agent_st/matlab

# Toolchain information
cc = C:\ProgramData\MATLAB\SupportPackages\R2022a\3P.instrset\mingw_w64.instrset\bin\gcc
cxx = C:\ProgramData\MATLAB\SupportPackages\R2022a\3P.instrset\mingw_w64.instrset\bin\g++
linker = C:\ProgramData\MATLAB\SupportPackages\R2022a\3P.instrset\mingw_w64.instrset\bin\gcc
linkerxx = C:\ProgramData\MATLAB\SupportPackages\R2022a\3P.instrset\mingw_w64.instrset\bin\g++

cflags =  -c -fexceptions -fno-omit-frame-pointer -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c99 -I "C:\Users\corra\Desktop\Università Trento\Master degree\2nd year\Intelligent vehicles and autonomous driving\Autonomous_driving_simulator\basic_agent_st\matlab\codegen\mex\a_opt_mex" -I "C:\Users\corra\Desktop\Università Trento\Master degree\2nd year\Intelligent vehicles and autonomous driving\Autonomous_driving_simulator\basic_agent_st\matlab" -I "C:\Users\corra\Desktop\Università Trento\Master degree\2nd year\Intelligent vehicles and autonomous driving\Autonomous_driving_simulator\basic_agent_st\matlab\codegen\mex\a_opt_mex\interface" -I "C:\Program Files\MATLAB\R2022a\extern\include" -I "." -DMODEL=a_opt_mex -D__USE_MINGW_ANSI_STDIO=1 -DMW_NEEDS_VERSION_H  

cxxflags =  -c -fexceptions -fno-omit-frame-pointer -std=c++11 -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c++11 -I "C:\Users\corra\Desktop\Università Trento\Master degree\2nd year\Intelligent vehicles and autonomous driving\Autonomous_driving_simulator\basic_agent_st\matlab\codegen\mex\a_opt_mex" -I "C:\Users\corra\Desktop\Università Trento\Master degree\2nd year\Intelligent vehicles and autonomous driving\Autonomous_driving_simulator\basic_agent_st\matlab" -I "C:\Users\corra\Desktop\Università Trento\Master degree\2nd year\Intelligent vehicles and autonomous driving\Autonomous_driving_simulator\basic_agent_st\matlab\codegen\mex\a_opt_mex\interface" -I "C:\Program Files\MATLAB\R2022a\extern\include" -I "." -DMODEL=a_opt_mex -D__USE_MINGW_ANSI_STDIO=1 -DMW_NEEDS_VERSION_H  

ldflags =  -m64 -Wl,--no-undefined -shared -static -L"C:\Program Files\MATLAB\R2022a\extern\lib\win64\mingw64" -llibmx -llibmex -llibmat -lm -llibmwlapack -llibmwblas -llibMatlabDataArray -llibMatlabEngine  -o "a_opt_mex.mexw64" -llibemlrt -llibcovrt -llibut -llibmwmathutil 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker @$out.rsp $ldflags
    rspfile = $out.rsp
    rspfile_content = $in

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/a_opt_mex_data.o : cxx $root/a_opt_mex_data.cpp
build $builddir/rt_nonfinite.o : cxx $root/rt_nonfinite.cpp
build $builddir/a_opt_mex_initialize.o : cxx $root/a_opt_mex_initialize.cpp
build $builddir/a_opt_mex_terminate.o : cxx $root/a_opt_mex_terminate.cpp
build $builddir/a_opt.o : cxx $root/a_opt.cpp
build $builddir/pass_primitive.o : cxx $root/pass_primitive.cpp
build $builddir/finalOptTime.o : cxx $root/finalOptTime.cpp
build $builddir/pass_primitive_1.o : cxx $root/pass_primitive_1.cpp
build $builddir/eml_setop.o : cxx $root/eml_setop.cpp
build $builddir/find.o : cxx $root/find.cpp
build $builddir/issorted.o : cxx $root/issorted.cpp
build $builddir/pass_primitivej0.o : cxx $root/pass_primitivej0.cpp
build $builddir/stop_primitive.o : cxx $root/stop_primitive.cpp
build $builddir/stop_primitivej0.o : cxx $root/stop_primitivej0.cpp
build $builddir/v_opt.o : cxx $root/v_opt.cpp
build $builddir/_coder_a_opt_mex_api.o : cxx $root/interface/_coder_a_opt_mex_api.cpp
build $builddir/_coder_a_opt_mex_mex.o : cxx $root/interface/_coder_a_opt_mex_mex.cpp
build $builddir/evalPrimitiveCoeffs.o : cxx $root/evalPrimitiveCoeffs.cpp
build $builddir/_coder_a_opt_mex_info.o : cxx $root/interface/_coder_a_opt_mex_info.cpp
build $builddir/cpp_mexapi_version.o : cxx $matlabdir/extern/version/cpp_mexapi_version.cpp

# Link
build $root\a_opt_mex.mexw64 : linkxx $builddir/a_opt_mex_data.o $builddir/rt_nonfinite.o $builddir/a_opt_mex_initialize.o $builddir/a_opt_mex_terminate.o $builddir/a_opt.o $builddir/pass_primitive.o $builddir/finalOptTime.o $builddir/pass_primitive_1.o $builddir/eml_setop.o $builddir/find.o $builddir/issorted.o $builddir/pass_primitivej0.o $builddir/stop_primitive.o $builddir/stop_primitivej0.o $builddir/v_opt.o $builddir/_coder_a_opt_mex_api.o $builddir/_coder_a_opt_mex_mex.o $builddir/evalPrimitiveCoeffs.o $builddir/_coder_a_opt_mex_info.o $builddir/cpp_mexapi_version.o 
